{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "site/build/distributions",
    "rewrites": [
      {
        "source": "/auth/gsuite/redirect",
        "function": "gsuiteAuthHandler"
      },
      {
        "source": "/auth/slack/redirect",
        "function": "slackAuthHandler"
      }
    ]
  },
  "functions": {
    "source": "server/functions"
  }
}
